@import "../common/index.wxss";
@import "../common/lib/weui.wxss";
@import "/wxSearch/wxSearch.wxss";

.search-section{
  background-color: #F1F1F1;
  padding: 5rpx 5rpx 10rpx;
}

.search-pancel{
  height: 58rpx;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  margin: 10rpx 10rpx;
  padding: 5rpx 20rpx;
  border-radius: 8rpx;
  align-items: center;
  font-size: 26rpx;
}

.search-img {
  width: 22rpx;
  height: 22rpx;
  margin: 0 10rpx 0 0;
}

.cancel-img {
  width: 32rpx;
  height: 32rpx;
  margin: 0 10rpx 0 0;
}

.nothing {
  text-align: center;
  margin-top: 100rpx;
  color: #bfbfbf;
  font-size: 28rpx;
  font-family: 微软雅黑;
}